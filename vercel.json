{
  "routes": [
    {
      "src": "/api/coworkings(.*)",
      "dest": "/api/coworkings.js"
    },
    {
      "src": "/api/cities",
      "dest": "/api/coworkings.js"
    },
    {
      "src": "/api/amenities",
      "dest": "/api/coworkings.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}